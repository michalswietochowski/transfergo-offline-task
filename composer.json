{
    "name": "michalswietochowski/transfergo-assessment",
    "description": "TransferGO Recruitment Offline Task",
    "authors": [
        {
            "name": "Michał Świętochowski",
            "email": "michalswietochowski@gmail.com"
        }
    ],
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "6.2.*",
        "symfony/dotenv": "6.2.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.2.*",
        "symfony/mailer": "6.2.*",
        "symfony/notifier": "6.2.*",
        "symfony/runtime": "6.2.*",
        "symfony/twig-bundle": "6.2.*",
        "symfony/twilio-notifier": "6.2.*",
        "symfony/yaml": "6.2.*",
        "twig/cssinliner-extra": "*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/inky-extra": "*",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "6.2.*",
        "symfony/css-selector": "6.2.*",
        "symfony/maker-bundle": "^1.48",
        "symfony/phpunit-bridge": "^6.2"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true,
        "platform": {
            "php": "8.2.1"
        }
    },
    "autoload": {
        "psr-4": {
            "TransferGO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TransferGO\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.2.*"
        }
    }
}
